<h2>Listing custom casts</h2>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>

      <th></th>
    </tr>
  </thead>
  <tbody>
<%= for cast <- @casts do %>
    <tr>
      <td><%= cast.title %></td>
      <td><%= Caster.Cast.downloaded?(cast) %></td>

      <td class="text-right">
        <%= cast_link(@conn, cast) %>
      </td>
    </tr>
<% end %>
  </tbody>
</table>
